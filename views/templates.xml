    <template id="search">
        <t t-call="website.layout">
            <t t-set="title">Search Results</t>
            <div class="oe_structure" />
            <div class="oe_structure">
                <div class="container">
                    <!-- <div>
                            <form role="form" action="/aspire360measures">
                                <button type="submit" class="btn btn-primary pull-left clearfix oe_login_buttons">Back</button>
                            </form>
                        </div>
                        <br/>
                        <br/> -->
                    <br />
                    <div>
                        <h1>Search Results</h1>
                    </div>
                    <div t-if="len(companies)==0">
                        <p>Sorry, no companies found..</p>
                    </div>
                    <div t-foreach="companies" t-as="company" class="alert alert-info mt16">
                        <!-- <form role="form" > -->
                        <form role="form" action="/aspire360measures/display_fundraise">
                            <input type="hidden" name="user_id" t-att-value="company.user_id" />
                            <button type="submit" class="btn btn-primary pull-right clearfix oe_login_buttons">Latest Fundraising Results</button>
                        </form>
                        <form role="form" action="/aspire360measures/display_sell">
                            <input type="hidden" name="user_id" t-att-value="company.user_id" />
                            <button type="submit" class="btn btn-primary pull-right clearfix oe_login_buttons">Latest Sell Results</button>
                        </form>

                        <p>
                            <h2 t-esc="company.company_name" />
                        </p>
                        <p>
                            <a>Founder: </a>
                            <a t-esc="company.name" />
                        </p>
                        <p>
                            <a>Industry: </a>
                            <a t-esc="company.company_industry" />
                        </p>
