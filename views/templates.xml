<odoo>
        <template id="e_index">
            <title>Aspire360Measures</title>
            <t t-call="website.layout">
                <t t-set="title">Aspire360Measures</t>
                <div class="oe_structure">
                    <div class="container">
                        <h1>Home Dashboard</h1>
                        <p><a t-attf-href="/aspire360measures/survey/fundraise">
                            Take the Readiness to Fundraise Survey</a>
                        </p>
                        <p><a t-attf-href="/aspire360measures/survey/sell">
                            Take the Readiness to Sell Survey</a>
                        </p>
                    </div>
                </div>
            </t>
        </template>

        <template id="v_index">
            <title>Aspire360Measures</title>
            <t t-call="website.layout">
                <t t-set="title">Aspire360Measures</t>
                <div class="oe_structure">
                    <div class="container">
                        <h1>Investor Homepage</h1>
                        <p><a t-attf-href="/aspire360measures/email">
                            Email Entrepreneurs</a>
                        </p>
                        <p>
                            TO BE UPDATED
                        </p>
                    </div>
                </div>
            </t>
        </template>
            
        <template id="email">
            <title>Aspire360Measures</title>
            <t t-call="website.layout">
                <t t-set="title">Aspire360Measures Email Sender</t>
                <div class="oe_structure">
                    <form target="_self" action="" method="post" id="email_form">

                        From: <input type="text" name="num_a" class="num_A"/><br/>

                        To: <input type="text" name="num_b" class="num_B"/><br/>

                        Subject: <input type="text" name="total" class="total" readonly="True"/><br/>

                         <button type="button" name="button" class="my-button">

                            <a href="/aspire360measures" class="btn btn-info">Submit</a>

                        </button>
                    </form>
                </div>
            </t>
        </template>
        
        <template id="setup">
            <title>Aspire360Measures</title>
            <t t-call="website.layout">
                <t t-set="title">Aspire360Measures</t>
                <div class="oe_structure">
                    <div class="container">
                        <h1>Are you an Entrepreneur or an Investor?</h1>
                        <p><a t-attf-href="/aspire360measures/setup/e">
                            Entrepreneur/CEO</a>
                        </p>
                        <p><a t-attf-href="/aspire360measures/setup/v">
                            Investor</a>
                        </p>
                    </div>
                </div>
            </t>
        </template>

        <template id="entrepreneur_form" name="Company Details">
            <t t-call="website.layout">
                <div id="wrap">
                    <div class="oe_structure">
                        <div class="container">
                            <form role="form" action="/aspire360measures/submit_info" method="POST">
                                <input type="hidden" name="company_info" t-att-value="request.csrf_token()"/>
                                <div class="form-group">
                                    <label for="company_name" class="control-label">Company Name </label>
                                    <input type="text" name="company_name" id="patient_name" class ="form-control" required="required"/>
                                </div>
                                <div class="form-group">
                                    <label for="company_industry">Please select the industry that your company belongs to:</label>
                                    <select id="company_industry" name="industry">
                                        <option value="Administrative Services">Administrative Services</option>
                                        <option value="Advertising">Advertising</option>
                                        <option value="Agriculture and Farming">Agriculture and Farming</option>
                                        <option value="Artificial Intelligence">Artificial Intelligence</option> 
                                        <option value="Biotechnology">Biotechnology</option> 
                                        <option value="Clothing and Apparel">Clothing and Apparel</option>  
                                        <option value="Commerce and Shopping">Commerce and Shopping</option>  
                                        <option value="Consumer Electronics">Consumer Electronics</option>
                                        <option value="Consumer Goods">Consumer Goods</option>  
                                        <option value="Content and Publishing">Content and Publishing</option>  
                                        <option value="Data and Analytics">Data and Analytics</option> 
                                        <option value="Design">Design</option> 
                                        <option value="Education">Education</option>  
                                        <option value="Energy">Energy</option> 
                                        <option value="Financial Services">Financial Services</option>     
                                        <option value="Food and Beverage">Food and Beverage</option>
                                        <option value="Gaming">Gaming</option>
                                        <option value="Government and Military">Government and Military</option>
                                        <option value="Hardware">Hardware</option>
                                        <option value="Healthcare">Healthcare</option>
                                        <option value="Information Technology">Information Technology</option>
                                        <option value="Manufacturing">Manufacturing</option>
                                        <option value="Media and Entertainment">Media and Entertainment</option>
                                        <option value="Navigation and Mapping">Navigation and Mapping</option>
                                        <option value="Others">Others</option>
                                        <option value="Payments">Payments</option>
                                        <option value="Platforms">Platforms</option>
                                        <option value="Privacy and Security">Privacy and Security</option>
                                        <option value="Professional Services">Professional Services</option>
                                        <option value="Real Estate">Real Estate</option>
                                        <option value="Sales and Marketing">Sales and Marketing</option>
                                        <option value="Science and Engineering">Science and Engineering</option>
                                        <option value="Software">Software</option>
                                        <option value="Sports">Sports</option>
                                        <option value="Sustainability">Sustainability</option>
                                        <option value="Telecommunications">Telecommunications</option>
                                        <option value="Transportation">Transportation</option>
                                        <option value="Travel and Tourism">Travel and Tourism</option>
                                        <option value="Video">Video</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label for="company_employees">Please indicate your company size:</label>
                                    <select id="company_employees" name="employees">
                                        <option value="1-10 employees">1-10 employees</option>
                                        <option value="11-50 employees">11-50 employees</option>
                                        <option value="51-200 employees">51-200 employees</option>
                                        <option value="200+ employees">200+ employees</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <label for="company_funding_stage">Please indicate what funding stage your company is in:</label>
                                    <select id="company_funding_stage" name="funding_stage">
                                        <option value="Pre-seed">Pre-seed</option>
                                        <option value="Seed">Seed</option>
                                        <option value="Stage A">Stage A</option>
                                        <option value="Stage B">Stage B</option>
                                        <option value="Stage C">Stage C</option>
                                        <option value="Stage D/E/Others">Stage D/E/Others</option>
                                    </select>
                                </div>
                                <div class="clearfix oe_login_buttons">
                                    <button type="submit" class="btn btn-primary pull-left">Submit</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </t>
        </template>


