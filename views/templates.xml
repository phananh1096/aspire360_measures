    <template id="contact_form" name="Contact Entrepreneur ">
        <xpath expr=".">
            <!-- <script type="text/javascript" src="/aspire360_measures/static/src/js/test.js"/> -->
            <link href="/aspire360_measures/static/src/css/styling.scss" rel="stylesheet" type="text/css" />
        </xpath>
        <t t-call="website.layout">
            <div id="wrap">
                <div class="oe_structure">
                    <div class="container">
                        <img src="/aspire360_measures/static/src/img/aspire360.png" style="margin-top:1rem; max-width: 15%; max-height: 15%;" class="w3-circle" id="aspire360_logo" />
                        <div class="row justify-content-center" style="margin-top:2rem; margin-bottom:2rem;">
                            <div class="col-8" style="font-family: Poppins; font-style: normal; font-weight: bold; text-align: center;">
                                <h1 id="setup_title">Founder Contact Form</h1>
                            </div>
                        </div>
                        <form role="form" action="/aspire360measures/send_message" method="POST">
                            <input type="hidden" name="user_id" t-att-value="company_id" />
                            <div class="form-row">
                                <div class="form-group col-md-6">
                                    <label for="email_recipient">To</label>
                                    <input type="text" name="email_recipient" class="form-control" id="email_recipient" t-att-value="to_name" placeholder="to_name" required="required" readonly="True" />
                                </div>
                                <div class="form-group col-md-6">
                                    <label for="email_sender" class="control-label">From</label>
                                    <input type="text" name="email_sender" class="form-control" id="email_sender" t-att-value="from_name" placeholder="from_name" required="required" readonly="True" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="message_subject" class="control-label">Message Subject</label>
                                <input type="text" name="message_subject" id="message_subject" class="form-control" required="required" />
                            </div>
                            <div class="form-group">
                                <label for="message_content" class="control-label">Message Content</label>
                                <textarea class="form-control" name="message_content" id="message_content" rows="5" required="required"></textarea>
                            </div>

                            <button type="submit" class="btn btn-primary">Send</button>
                        </form>
