<odoo>
    <template id="e_index">
        <title>Aspire360Measures</title>
        <t t-call="website.layout">
            <t t-set="title">Aspire360Measures</t>
            <div class="oe_structure">
                <div class="container" style="margin-top: 5%">
                    <div style="width: 100%;">
                        <div class="col-8" style="font-family: Poppins; font-style: normal; font-weight: bold; text-align: left;">
                            <img src="/aspire360_measures/static/src/img/aspire360.png" style="margin-top:1rem; max-width: 15%; max-height: 15%;" class="w3-circle" id="aspire360_logo" />
                            <h1 id="setup_title">Welcome Back!</h1>
                        </div>
                        <div style="margin-left: 88%; height: 100px;">
                            <p style="color:grey;">
                                <script> document.write(new Date().toLocaleDateString()); </script>
                            </p>
                        </div>
                    </div>

                    <div class="row justify-content-center" style="margin-bottom:3rem;">
                        <div class="card" style="width: 25rem; height: 20rem;margin-right: 1rem;">
                            <div class="card-body">
                                <img src="/aspire360_measures/static/src/img/fundraise_logo.png" width = "20%" style="width: 50%" class="card-img-top" alt="..." />
                                <h4 class="card-title">Readiness to FUNDRAISE</h4>
                                <a t-attf-href="/aspire360measures/survey/fundraise" class="btn btn-primary stretched-link">Start Assessment</a>
                            </div>
                        </div>
                    
                        <div class="card" style="width: 25rem; height: 20rem;">
                            <div class="card-body">
                                <img src="/aspire360_measures/static/src/img/sell_icon.png" width = "20%" style="width: 50%" class="card-img-top" alt="..." />
                                <h4 class="card-title">Readiness to SELL</h4>
                                <a t-attf-href="/aspire360measures/survey/sell" class="btn btn-primary stretched-link">Start Assessment</a>
                            </div>
                        </div>
                        <div style="margin-left: 8.5%; height: 100px;float: left;">
                            <form role="form" action="/aspire360measures/entrepreneur_edit_profile">
                                <div class="clearfix oe_login_buttons">
                                    <button type="submit" class="btn btn-primary pull-left" style="background-color: Transparent;background-repeat:no-repeat;border: none;cursor:pointer;overflow: hidden;outline:none;color:grey">Edit Profile</button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </t>
    </template>

