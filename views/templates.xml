        <template id="survey_error">
            <title>Aspire360Measures</title>
            <t t-call="website.layout">
                <t t-set="title">Error, no survey results found</t>
                <div class="oe_structure">
                    <div class="container">
                        <h3>Sorry, this company has not completed this survey yet..</h3>
                        <br/>
                        <form role="form" action="/aspire360measures/search">    
                            <button type="submit" class="btn btn-primary pull-bottom clearfix oe_login_buttons">Back</button>
                        </form>
                    </div>
                </div>
            </t>
        </template>

